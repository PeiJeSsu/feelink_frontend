{
  "name": "feelink_frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@erase2d/fabric": "^1.1.7",
    "@mui/styles": "^6.4.6",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.4.7",
    "@mui/material": "^6.4.7",
    "axios": "^1.8.3",
    "cra-template": "1.2.0",
    "fabric": "^6.6.1",
    "file-saver": "^2.0.5",
    "jspdf": "^3.0.0",
    "pdfjs-dist": "^5.0.375",
    "react": "^19.0.0",
    "react-color": "^2.19.3",
    "react-colorful": "^5.6.1",
    "react-dom": "^19.0.0",
    "react-draggable": "^4.4.6",
    "react-hotkeys-hook": "^4.6.1",
    "react-scripts": "5.0.1",
    "uuid": "^11.1.0",
    "web-vitals": "^4.2.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "prettier": "3.5.3",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/preset-env": "^7.26.9",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "jest": "^27.5.1",
    "jest-fetch-mock": "^3.0.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "coveragePathIgnorePatterns": [
      "src/index.js",
      "src/reportWebVitals.js",
      "src/App.test.js",
      "src/App.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "transform": {
    "^.+\\.[t|j]sx?$": "babel-jest"
  },
  "transformIgnorePatterns": [
    "node_modules/(?!(axios)/)"
  ]
  }
}

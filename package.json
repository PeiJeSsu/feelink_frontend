{
	"name": "feelink_frontend",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"@emotion/react": "^11.14.0",
		"@emotion/styled": "^11.14.0",
		"@erase2d/fabric": "^1.1.7",
		"@mui/icons-material": "^6.4.7",
		"@mui/material": "^6.4.7",
		"@testing-library/user-event": "^14.6.1",
		"axios": "^1.8.3",
		"fabric": "^6.6.1",
		"file-saver": "^2.0.5",
		"firebase": "^12.0.0",
		"i18next": "^23.15.1",
		"jquery": "^3.7.1",
		"markdown-it": "^14.1.0",
		"prop-types": "^15.8.1",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"react-i18next": "^13.5.0",
		"react-resizable": "^3.0.5",
		"react-router-dom": "^7.7.1",
		"react-scripts": "5.0.1",
		"uuid": "^11.1.0"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"test:coverage": "react-scripts test --coverage --watchAll=false --passWithNoTests",
		"test:coverage:ci": "react-scripts test --coverage --watchAll=false --passWithNoTests; node scripts/fix-lcov-paths.js",
		"test:file": "react-scripts test --watchAll=false --testPathPattern",
		"test:report": "react-scripts test --coverage --watchAll=false --testResultsProcessor=jest-stare",
		"test:view-report": "jest-stare",
		"eject": "react-scripts eject"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@babel/plugin-proposal-private-property-in-object": "^7.21.11",
		"@babel/preset-env": "^7.26.9",
		"@babel/preset-react": "^7.26.3",
		"@eslint/css": "^0.10.0",
		"@eslint/js": "^9.33.0",
		"@eslint/json": "^0.13.1",
		"@eslint/markdown": "^7.1.0",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/react": "^16.2.0",
		"eslint": "^8.57.1",
		"eslint-plugin-react": "^7.37.5",
		"globals": "^16.3.0",
		"jest": "^29.7.0",
		"jest-environment-jsdom": "^29.7.0",
		"jest-fetch-mock": "^3.0.3",
		"jest-stare": "^2.5.2",
		"prettier": "3.5.3"
	},
	"jest": {
		"collectCoverageFrom": [
			"src/**/*.{js,jsx,ts,tsx}"
		],
		"coveragePathIgnorePatterns": [
			"src/index.js",
			"src/reportWebVitals.js",
			"src/App.test.js",
			"src/App.js",
			"src/setupTests.js"
		],
		"coverageReporters": [
			"text",
			"lcov",
			"html"
		],
		"coverageThreshold": {
			"global": {
				"branches": 80,
				"functions": 80,
				"lines": 80,
				"statements": 80
			}
		},
		"transform": {
			"^.+\\.[t|j]sx?$": "babel-jest"
		},
		"transformIgnorePatterns": [
			"node_modules/(?!(axios|@erase2d/fabric|react-router-dom|react-router)/)"
		]
	}
}
